{
  "[dockercompose]": {
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.insertSpaces": true,
    "editor.quickSuggestions": {
      "comments": false,
      "other": true,
      "strings": true
    },
    "editor.tabSize": 2
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-containers"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[helm]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[xml]": {
    "editor.formatOnSave": false
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "chat.commandCenter.enabled": false,
  "debug.showInStatusBar": "never",
  "debug.toolBarLocation": "hidden",
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "editor.bracketPairColorization.enabled": false,
  "editor.detectIndentation": false,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.multiCursorPaste": "full",
  "editor.renderWhitespace": "none",
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "variable.readonly": {
        "foreground": "#9CDCFE"
      }
    }
  },
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "functions": "#ffcc00"
  },
  "emmet.showExpandedAbbreviation": "never",
  "errorLens.scrollbarHackEnabled": true,
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    ".stignore": "ignore",
    "*.Jenkinsfile": "groovy",
    "Jenkinsfile": "groovy",
    ".zsh_plugins.txt": "shellscript"
  },
  "files.insertFinalNewline": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.ignoreLimitWarning": true,
  "git.inputValidationLength": 120,
  "git.inputValidationSubjectLength": 120,
  "git.openRepositoryInParentFolders": "never",
  "git.pruneOnFetch": true,
  "git.suggestSmartCommit": false,
  "gitlens.advanced.messages": {
    "suppressCreatePullRequestPrompt": true
  },
  "gitlens.advanced.similarityThreshold": 50,
  "gitlens.blame.ignoreWhitespace": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.scrollable": false,
  "gitlens.launchpad.indicator.enabled": false,
  "gitlens.liveshare.allowGuestAccess": false,
  "gitlens.views.branches.branches.layout": "list",
  "gitlens.views.scm.grouped.views": {
    "branches": false,
    "commits": false,
    "contributors": false,
    "launchpad": false,
    "remotes": false,
    "repositories": false,
    "searchAndCompare": false,
    "stashes": false,
    "tags": false,
    "worktrees": false
  },
  "go.toolsManagement.autoUpdate": true,
  "html.format.wrapAttributes": "force-aligned",
  "javascript.preferences.importModuleSpecifier": "relative",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "material-icon-theme.files.associations": {
    ".editorconfig": "settings",
    "Justfile": "makefile"
  },
  "material-icon-theme.folders.associations": {
    ".transformer": "environment",
    ".vs": "vscode",
    "obj": "dist",
    "packageSource": "packages"
  },
  "material-icon-theme.folders.color": "#90a4ae",
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.hidesExplorerArrows": true,
  "redhat.telemetry.enabled": false,
  "remote.autoForwardPortsSource": "hybrid",
  "remote.SSH.remotePlatform": {
    "control-0.k8s.home": "linux"
  },
  "resmon.show.battery": false,
  "resmon.show.cpufreq": false,
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.lens.references.adt.enable": true,
  "rust-analyzer.lens.references.enumVariant.enable": true,
  "rust-analyzer.lens.references.method.enable": true,
  "rust-analyzer.lens.references.trait.enable": true,
  "rust-analyzer.restartServerOnConfigChange": true,
  "search.followSymlinks": false,
  "svelte.enable-ts-plugin": true,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.commandsToSkipShell": [
    "workbench.scm.focus",
    "workbench.view.explorer"
  ],
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.profiles.windows": {
    "C:\\Program Files\\Git\\bin\\bash.exe (migrated)": {
      "args": [],
      "path": "C:\\Program Files\\Git\\bin\\bash.exe"
    }
  },
  "terminal.integrated.shellIntegration.enabled": false,
  "terminal.integrated.tabs.hideCondition": "never",
  "terminal.integrated.windowsEnableConpty": false,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.referencesCodeLens.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vs-kubernetes": {
    "vs-kubernetes.crd-code-completion": "enabled",
    "vs-kubernetes.knownKubeconfigs": [
      "c:\\Users\\dev\\.kube\\config",
      "/home/dev/.kube/config"
    ],
    "vs-kubernetes.kubeconfig": "/home/dev/.kube/config"
  },
  "window.newWindowDimensions": "inherit",
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}${separator}${appName}",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "workbench.startupEditor": "none",
  "yaml.format.bracketSpacing": false,
  "yaml.format.singleQuote": true,
